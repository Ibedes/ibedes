            <h2 class=\"text-5xl md:text-7xl font-black uppercase tracking-tighter\">\n                        Output<span class=\"text-lime-500\">_Terakhir</span>\n                    </h2>\n                    <div class=\"hidden md:block text-right\">\n                        <div class=\"text-xs text-lime-500 mb-1\">DATA_STREAM: ONLINE</div>\n                        <div class=\"w-32 h-2 bg-stone-800 relative overflow-hidden\">\n                            <div class=\"absolute inset-0 bg-lime-500 w-1/2 animate-loading-bar\"></div>\n                        </div>\n                    </div>\n                </div>\n\n                <div class=\"grid grid-cols-1 md:grid-cols-2 gap-8\">\n                    <!-- Project Cards (Repeated for effect) -->\n                    <div class=\"group relative aspect-video bg-stone-900 border-2 border-stone-800 overflow-hidden cursor-trigger\">\n                        <div class=\"absolute inset-0 bg-stone-800 flex items-center justify-center group-hover:scale-105 transition-transform duration-500\">\n                            <div class=\"text-stone-700 font-black text-9xl select-none\">IMG_01</div>\n                        </div>\n                        <div class=\"absolute inset-0 bg-lime-500/0 group-hover:bg-lime-500/20 transition-colors duration-300 flex flex-col justify-end p-6\">\n                            <div class=\"transform translate-y-full group-hover:translate-y-0 transition-transform duration-300 bg-black border border-lime-500 p-4 max-w-xs\">\n                                <h4 class=\"text-xl font-bold text-lime-500 mb-1\">PROYEK ALPHA 1</h4>\n                                <p class=\"text-xs text-stone-400\">DESAIN / PENGEMBANGAN / DESTRUKSI</p>\n                            </div>\n                        </div>\n                        <div class=\"absolute top-0 left-0 w-4 h-4 border-t-4 border-l-4 border-lime-500 opacity-0 group-hover:opacity-100 transition-opacity\"></div>\n                        <div class=\"absolute bottom-0 right-0 w-4 h-4 border-b-4 border-r-4 border-lime-500 opacity-0 group-hover:opacity-100 transition-opacity\"></div>\n                    </div>\n                    <!-- ... Add 3 more project cards (omitted for brevity in template, but included in complete code) -->\n                    <div class=\"group relative aspect-video bg-stone-900 border-2 border-stone-800 overflow-hidden cursor-trigger\">\n                        <div class=\"absolute inset-0 bg-stone-800 flex items-center justify-center group-hover:scale-105 transition-transform duration-500\">\n                            <div class=\"text-stone-700 font-black text-9xl select-none\">IMG_02</div>\n                        </div>\n                        <div class=\"absolute inset-0 bg-lime-500/0 group-hover:bg-lime-500/20 transition-colors duration-300 flex flex-col justify-end p-6\">\n                            <div class=\"transform translate-y-full group-hover:translate-y-0 transition-transform duration-300 bg-black border border-lime-500 p-4 max-w-xs\">\n                                <h4 class=\"text-xl font-bold text-lime-500 mb-1\">PROYEK ALPHA 2</h4>\n                                <p class=\"text-xs text-stone-400\">DESAIN / PENGEMBANGAN / DESTRUKSI</p>\n                            </div>\n                        </div>\n                        <div class=\"absolute top-0 left-0 w-4 h-4 border-t-4 border-l-4 border-lime-500 opacity-0 group-hover:opacity-100 transition-opacity\"></div>\n                        <div class=\"absolute bottom-0 right-0 w-4 h-4 border-b-4 border-r-4 border-lime-500 opacity-0 group-hover:opacity-100 transition-opacity\"></div>\n                    </div>\n                    <div class=\"group relative aspect-video bg-stone-900 border-2 border-stone-800 overflow-hidden cursor-trigger\">\n                        <div class=\"absolute inset-0 bg-stone-800 flex items-center justify-center group-hover:scale-105 transition-transform duration-500\">\n                            <div class=\"text-stone-700 font-black text-9xl select-none\">IMG_03</div>\n                        </div>\n                        <div class=\"absolute inset-0 bg-lime-500/0 group-hover:bg-lime-500/20 transition-colors duration-300 flex flex-col justify-end p-6\">\n                            <div class=\"transform translate-y-full group-hover:translate-y-0 transition-transform duration-300 bg-black border border-lime-500 p-4 max-w-xs\">\n                                <h4 class=\"text-xl font-bold text-lime-500 mb-1\">PROYEK ALPHA 3</h4>\n                                <p class=\"text-xs text-stone-400\">DESAIN / PENGEMBANGAN / DESTRUKSI</p>\n                            </div>\n                        </div>\n                        <div class=\"absolute top-0 left-0 w-4 h-4 border-t-4 border-l-4 border-lime-500 opacity-0 group-hover:opacity-100 transition-opacity\"></div>\n                        <div class=\"absolute bottom-0 right-0 w-4 h-4 border-b-4 border-r-4 border-lime-500 opacity-0 group-hover:opacity-100 transition-opacity\"></div>\n                    </div>\n                    <div class=\"group relative aspect-video bg-stone-900 border-2 border-stone-800 overflow-hidden cursor-trigger\">\n                        <div class=\"absolute inset-0 bg-stone-800 flex items-center justify-center group-hover:scale-105 transition-transform duration-500\">\n                            <div class=\"text-stone-700 font-black text-9xl select-none\">IMG_04</div>\n                        </div>\n                        <div class=\"absolute inset-0 bg-lime-500/0 group-hover:bg-lime-500/20 transition-colors duration-300 flex flex-col justify-end p-6\">\n                            <div class=\"transform translate-y-full group-hover:translate-y-0 transition-transform duration-300 bg-black border border-lime-500 p-4 max-w-xs\">\n                                <h4 class=\"text-xl font-bold text-lime-500 mb-1\">PROYEK ALPHA 4</h4>\n                                <p class=\"text-xs text-stone-400\">DESAIN / PENGEMBANGAN / DESTRUKSI</p>\n                            </div>\n                        </div>\n                        <div class=\"absolute top-0 left-0 w-4 h-4 border-t-4 border-l-4 border-lime-500 opacity-0 group-hover:opacity-100 transition-opacity\"></div>\n                        <div class=\"absolute bottom-0 right-0 w-4 h-4 border-b-4 border-r-4 border-lime-500 opacity-0 group-hover:opacity-100 transition-opacity\"></div>\n                    </div>\n\n                </div>\n            </div>\n        </section>\n\n        <!-- Rugged Footer -->\n        <footer class=\"bg-lime-500 text-black pt-20 pb-10 px-6 mt-12 overflow-hidden relative\">\n            <div class=\"absolute top-0 left-0 w-full h-4 bg-repeat-x\" style=\"background-image: linear-gradient(45deg, #000 25%, transparent 25%, transparent 50%, #000 50%, #000 75%, transparent 75%, transparent); background-size: 20px 20px;\"></div>\n            \n            <div class=\"max-w-7xl mx-auto grid grid-cols-1 md:grid-cols-2 gap-12\">\n                <div>\n                    <h2 class=\"text-6xl md:text-8xl font-black mb-6 leading-none\">\n                        MULAI<br>SEKARANG.\n                    </h2>\n                    <button class=\"bg-black text-lime-500 px-8 py-4 font-bold uppercase hover:bg-white hover:text-black transition-all shadow-[8px_8px_0px_0px_rgba(0,0,0,1)] hover:shadow-none hover:translate-x-2 hover:translate-y-2 cursor-trigger\">\n                        Kirim Transmisi\n                    </button>\n                </div>\n                \n                <div class=\"flex flex-col justify-end items-start md:items-end font-bold space-y-2\">\n                    <a href=\"#\" class=\"hover:underline decoration-4 cursor-trigger\">INSTAGRAM_</a>\n                    <a href=\"#\" class=\"hover:underline decoration-4 cursor-trigger\">LINKEDIN_</a>\n                    <a href=\"#\" class=\"hover:underline decoration-4 cursor-trigger\">TWITTER_X</a>\n                    <div class=\"mt-8 text-sm opacity-75 font-mono\">\n                        © 2024 SEKTOR_7 INDUSTRIES.<br>\n                        SEMUA HAK DILINDUNGI OLEH HUKUM FISIKA.\n                    </div>\n                </div>\n            </div>\n        </footer>\n\n    </div>\n";
const cssContent = "";
const jsContent = `
document.addEventListener('DOMContentLoaded', () => {
    const customCursor = document.getElementById('custom-cursor');
    const cursorOutline = document.getElementById('cursor-outline');
    const menuButton = document.getElementById('menu-button');
    const mobileMenu = document.getElementById('mobile-menu');
    const terminalIcon = document.getElementById('menu-icon-terminal');
    const xIcon = document.getElementById('menu-icon-x');
    const menuLinks = mobileMenu ? mobileMenu.querySelectorAll('a') : [];

    document.addEventListener('mousemove', (e) => {
        if (!customCursor) return;
        customCursor.style.transform = 'translate(' + (e.clientX - 16) + 'px, ' + (e.clientY - 16) + 'px)';
    });

    function toggleCursorHover(isHovering) {
        if (!customCursor || !cursorOutline) return;
        if (isHovering) {
            customCursor.classList.add('is-hovering');
        } else {
            customCursor.classList.remove('is-hovering');
        }
    }

    document.querySelectorAll('.cursor-trigger, button, a').forEach((el) => {
        el.addEventListener('mouseenter', () => toggleCursorHover(true));
        el.addEventListener('mouseleave', () => toggleCursorHover(false));
    });
    
    let isMenuOpen = false;

    function toggleMenu() {
        if (!mobileMenu || !terminalIcon || !xIcon) return;
        isMenuOpen = !isMenuOpen;
        if (isMenuOpen) {
            mobileMenu.classList.remove('hidden');
            mobileMenu.classList.add('flex');
            terminalIcon.classList.add('hidden');
            xIcon.classList.remove('hidden');
        } else {
            mobileMenu.classList.add('hidden');
            mobileMenu.classList.remove('flex');
            terminalIcon.classList.remove('hidden');
            xIcon.classList.add('hidden');
        }
    }

    menuButton?.addEventListener('click', toggleMenu);

    menuLinks.forEach((link) => {
        link.addEventListener('click', toggleMenu);
    });
});
`;
